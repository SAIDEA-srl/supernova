{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day"
        }
      }
    ]    
  },
  "AllowedHosts": "*",
  "InfluxDB": {
    "Url": "___INFLUX_URL___",
    "Token": "____INFLUX_API_TOKEN____",
    "Database": "pvs_bolzano",
    "Organization": "Saidea"
  },
  "OpenId": {
    "Authority": "https://identityeurac.goantares.uno"
  },
  "RabbitMQ": {
    "Host": "__RABBITMQ_HOST__",
    "OAuth": {
      "ClientId": "__OAUTH_RABBITMQ_CLIENT_ID__",
      "ClientSecret": "__OAUTH_RABBITMQ_CLIENT_SECRET__"
    }
  },
  "Reports": {
    "BasePath": "__REPORTS_BASE_PATH__"
  }
}
